{
	"name": "multiconfigbuild",
	"publisher": "MaximKolesnik",
	"displayName": "MultiConfigurationBuild",
	"description": "Provide ability to manage multiple configurations of a source tree",
	"repository": "https://github.com/MaximKolesnik/multiconfigbuild",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.multiConfigurationBuild",
				"title": "Multiple Configurations Build",
				"category": "MultiConfigurationBuild"
			},
			{
				"command": "extension.build",
				"title": "Build",
				"category": "MultiConfigurationBuild"
			},
			{
				"command": "extension.debug",
				"title": "Debug",
				"category": "MultiConfigurationBuild"
			},
			{
				"command": "extension.run",
				"title": "Run",
				"category": "MultiConfigurationBuild"
			},
			{
				"command": "extension.buildCurrentFile",
				"title": "Build Current File",
				"category": "MultiConfigurationBuild"
			}
		],
		"keybindings": [
			{
				"command": "extension.multiConfigurationBuild",
				"key": "ctrl+shift+m"
			},
			{
				"command": "extension.buildAndDebug",
				"key": "ctrl+shift+d"
			},
			{
				"command": "extension.run",
				"key": "ctrl+shift+r"
			},
			{
				"command": "extension.buildCurrentFile",
				"key": "ctrl+shift+b"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.0-next.0"
	}
}
